{
  "manifest_version": 3,

  "name": "Phoenix Proxy FF",
  "description": "An extension for using proxy service in Mozilla Firefox",
  "version": "1.1",
  "author": "Phoenix Whitebrow",

  "browser_specific_settings": {
    "gecko": {
      "id": "phoenix-proxy-ff@phoenixvs.site",
      "strict_min_version": "91.1.0"
    }
  },

  "permissions": [
    "proxy"
  ],

  "icons": {
    "16":"images/icon-16.png",
    "32":"images/icon-32.png",
    "48":"images/icon-48.png",
    "128":"images/icon-128.png"
  },

  "action": {
    "default_popup":"popup.html"
  }
}